<form method="post" enctype="multipart/form-data" asp-controller="Object" asp-action="Post">
    <div class="form-group">
        <div class="col-md-10">
            <p>Upload one or more files using this form:</p>
            <input  type="file" name="request"  />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Upload" />
        </div>
    </div>
</form>


<img src="@Url.Action("GetImage")" />